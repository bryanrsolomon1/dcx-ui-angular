{
    "development": {
      "client": {
        "ENVIRONMENT": "development"
      },
      "gulp": {
        "env": "dev",
        "dist": "./dist.dev/",
        "distApp": "./dist.dev/app/",
        "distBower": "./dist.dev/bower_components/"
      }
    },
    "integration": {
      "client": {
        "ENVIRONMENT": "integration"
      },
      "gulp": {
        "env": "itg",
        "dist": "./dist.pro/",
        "distApp": "./dist.pro/app/",
        "distBower": "./dist.pro/bower_components/"
      }
    },
    "production": {
      "client": {
        "ENVIRONMENT": "production"
      },
      "gulp": {
        "env": "pro",
        "dist": "./dist.pro/",
        "distApp": "./dist.pro/app/",
        "distBower": "./dist.pro/bower_components/"
      }
    }
}